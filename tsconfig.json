{
	"compileOnSave": false,
	"compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "lib": [
            "es2017",
            "dom"
        ]
    },
    "exclude": [
        "node_modules"
	]
}
